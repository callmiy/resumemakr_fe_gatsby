{
  "name": "resumemakr",
  "private": true,
  "description": "Resumemakr is platform for building career propelling resumes",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "build:staging": "env-cmd -e staging yarn build",
    "build:production": "env-cmd -e production yarn build",
    "develop": "env-cmd -e dev gatsby develop --port 3023",
    "start": "yarn develop",
    "start:e2e": "env-cmd -e staging gatsby develop --port 3023",
    "start:staging": "serve --no-clipboard --listen 3022 public",
    "start-build-staging": "yarn build:staging && yarn start:staging",
    "gatsby-serve": "gatsby serve --port 3022",
    "gatsby-build-serve": "yarn build:staging && yarn gatsby-serve",
    "test": "env-cmd -e dev jest --no-cache --watch --runInBand",
    "test:coverage": "env-cmd -e dev jest --coverage --runInBand",
    "test-e2e-run-staging": "start-server-and-test start-build-staging :3022 cy-run-staging",
    "test-e2e-open-staging": "start-server-and-test start-build-staging :3022 cy-open-staging",
    "type-check": "tsc --noEmit",
    "lint": "tslint --project .",
    "gen-apollo-types": "apollo codegen:generate --endpoint=http://localhost:4023/api --tagName=gql --target=typescript --includes=src/graphql/apollo/*.ts --outputFlat=src/graphql/apollo/types",
    "gen-gatsby-types": "apollo codegen:generate --endpoint=http://localhost:3023/___graphql --tagName=graphql --target=typescript --includes=src/**/*.ts* --outputFlat=src/graphql/gatsby/types",
    "cy-open-dev": "cypress open --config baseUrl=http://localhost:3023/",
    "cy-open-staging": "cypress open --config baseUrl=http://localhost:3022/",
    "cy-run-dev": "cypress run --config baseUrl=http://localhost:3023/",
    "cy-run-staging": "env-cmd -e staging cypress run --config baseUrl=http://localhost:3022/",
    "build-deploy": "yarn build:production && yarn deploy",
    "deploy": "netlify deploy --prod --dir=public"
  },
  "dependencies": {
    "@absinthe/socket": "^0.2.1",
    "@absinthe/socket-apollo-link": "^0.2.1",
    "apollo": "^2.6.2",
    "apollo-boost": "^0.3.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link-state": "^0.4.2",
    "env-cmd": "9.0.1",
    "formik": "^1.5.1",
    "gatsby": "^2.2.13",
    "gatsby-image": "^2.0.35",
    "gatsby-plugin-create-client-paths": "^2.0.5",
    "gatsby-plugin-env-variables": "^1.0.1",
    "gatsby-plugin-google-analytics": "^2.0.18",
    "gatsby-plugin-manifest": "^2.0.24",
    "gatsby-plugin-netlify": "^2.0.13",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.11",
    "gatsby-plugin-sass": "^2.0.11",
    "gatsby-plugin-sharp": "^2.0.32",
    "gatsby-plugin-typescript": "^2.0.11",
    "gatsby-source-filesystem": "^2.0.28",
    "gatsby-transformer-sharp": "^2.1.17",
    "immutability-helper": "^3.0.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "phoenix": "^1.4.2",
    "polished": "^3.1.0",
    "react": "16.9.0-alpha.0",
    "react-apollo": "^2.5.3",
    "react-dom": "16.9.0-alpha.0",
    "react-helmet-async": "^1.0.2",
    "react-transition-group": "^2.7.1",
    "semantic-ui-less": "^2.4.1",
    "semantic-ui-react": "^0.86.0",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.0.3",
    "@types/graphql": "^14.0.7",
    "@types/isomorphic-fetch": "^0.0.35",
    "@types/jest": "^24.0.11",
    "@types/lodash": "^4.14.123",
    "@types/phoenix": "^1.4.0",
    "@types/reach__router": "^1.2.3",
    "@types/react": "^16.8.9",
    "@types/react-dom": "^16.8.3",
    "@types/react-transition-group": "^2.0.16",
    "@types/yup": "^0.26.12",
    "babel-preset-gatsby": "^0.1.10",
    "cypress": "^3.2.0",
    "cypress-file-upload": "^3.0.9",
    "cypress-testing-library": "^2.3.6",
    "eslint-plugin-graphql": "^3.0.3",
    "gatsby-plugin-alias-imports": "^1.0.5",
    "gatsby-plugin-less": "^2.0.12",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.5.0",
    "jest-dom": "^3.1.3",
    "less": "^3.9.0",
    "prettier": "^1.16.4",
    "react-testing-library": "^6.0.3",
    "serve": "11.0.0",
    "start-server-and-test": "^1.7.12",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.3.3",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.3.4000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-hello-world"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
